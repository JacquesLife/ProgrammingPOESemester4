@{
    ViewData["Title"] = "Processing";
}
<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
    <meta charset="UTF-8">
    <title>Claims List</title>
    <link rel="stylesheet" href="style.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
    <!-- Claims list container -->
    <div class="form-body">
        <div class="form-container">
            <div class="title">Submitted Claims</div>
            <div class="claims-list">
                <div class="claim-item">
                    <h3 class="claim-title">Claim #1</h3>
                    <p class="claim-description">Lorem ipsum dolor sit amet consectetur adipisicing elit. Maxime ab sequi dolore incidunt recusandae iure, alias eveniet totam tempora voluptates mollitia quod libero? Rem pariatur vero voluptates a maxime sunt?</p>
                    <span class="claim-date">Submitted on: September 8, 2024</span>
                    <span class="claim-status">Status: Pending</span>
                    <button class="track-button">Track</button>
                    <button class="approve-button">Approve</button>
                    <button class="reject-button">Reject</button>
                </div>
                <div class="claim-item">
                    <h3 class="claim-title">Claim #2</h3>
                    <p class="claim-description">Lorem ipsum dolor sit amet consectetur adipisicing elit. Neque impedit atque blanditiis, omnis deleniti in enim esse optio quam, totam doloribus recusandae modi iusto minus quaerat natus fugit sed voluptatem.</p>
                    <span class="claim-date">Submitted on: September 7, 2024</span>
                    <span class="claim-status">Status: Approved</span>
                    <button class="track-button">Track</button>
                    <button class="approve-button">Approve</button>
                    <button class="reject-button">Reject</button>
                </div>
                <!-- Add more claim items as needed -->
            </div>
        </div>
    </div>
</body>
</html>
